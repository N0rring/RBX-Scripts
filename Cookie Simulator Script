local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/GreenDeno/Venyx-UI-Library/main/source.lua"))()
local venyx = library.new("Cookie Simulator", 5013109572)
local page = venyx:addPage("AutoFarm", 5012544693)
local section1 = page:addSection("AutoClick")
section1:addButton("AutoClick On", function()
    getgenv().autoclick = true
while getgenv().autoclick == true do

local args = {
    [1] = game:GetService("Players").LocalPlayer
}
game:GetService("ReplicatedStorage").clickEventWall:FireServer(unpack(args))


wait()
end

end)
section1:addButton("AutoClick Off", function()
    getgenv().autoclick = false
print("Clicked")
end)

-----------------
 local page = venyx:addPage("AutoBuy BackPack", 5012544693)
 local section2 = page:addSection("AutoBuy BackPack")
 section2:addButton("AutoBuy BackPack On", function()
    getgenv().autoBuyBackPack = true
while getgenv().autoBuyBackPack == true do
    
 game:GetService("ReplicatedStorage").Buy:FireServer()
 
 wait()
end 
end)
section2:addButton("AutoBuy BackPack Off", function()
 getgenv().autoBuyBackPack = false 
end)


 local section3 = page:addSection("AutoBuy Per Click")
 section3:addButton("AutoBuy PerClick On", function()
    getgenv().autoBuyPerClick = true
while getgenv().autoBuyPerClick == true do
    
 game:GetService("ReplicatedStorage").BuyClick:FireServer()
 
 wait()
end 
end)
section3:addButton("AutoBuy PerClick Off", function()
 getgenv().autoBuyPerClick = false 
end)







local section4 = page:addSection("AutoBuy Rebirths")
 section4:addButton("AutoBuy Rebirths On", function()
    getgenv().autoBuyPerClick = true
while getgenv().autoBuyPerClick == true do
    
 game:GetService("ReplicatedStorage").BuyRank:FireServer()
 
 wait()
end 
end)
section4:addButton("AutoBuy Rebirths Off", function()
 getgenv().autoBuyPerClick = false 
 end)





 
 
 
 
 
 local page = venyx:addPage("Misc", 5012544693)


    local section1 = page:addSection("Toggle UI Keybind")
    section1:addKeybind("Toggle Keybind", Enum.KeyCode.F, function()
print("Activated Keybind")
venyx:toggle()
end, function()
print("Changed Keybind")
end)
local section2 = page:addSection("AntiAfk")
section2:addButton("AntiAfk", function()
    local VirtualUser=game:service'VirtualUser'
game:service'Players'.LocalPlayer.Idled:connect(function()
VirtualUser:CaptureController()
VirtualUser:ClickButton2(Vector2.new())
end)

warn("Anti-Afk has Loaded")
    print("Clicked")
    end)
